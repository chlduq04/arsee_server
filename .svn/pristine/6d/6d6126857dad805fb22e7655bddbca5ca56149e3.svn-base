.management_main {
	width: 100%;
	height: 100%;
}

.node circle {
	cursor: pointer;
	fill: #fff;
	stroke: steelblue;
	stroke-width: 1.5px;
}

.node text {
	font-size: 11px;
}

path.link {
	fill: none;
	stroke: #ccc;
	stroke-width: 1.5px;
}

.manage-else {
	float: right;
	margin-right: 2%;
}

#manage-depth-buttons {
	float: left;
}

#manage-depth-buttons-option {
	float: left;
	margin: 0px 10px;
}

.btn.btn-default {
	margin: 0px 1px;
}

.manage-depth-dropdown {
	width: 200px;
}

.manage-submit {
	height: 25px;
	width: 50px !important;
	float: right;
	line-height: 5px;
	margin-top: 10px;
}

.manage-modify {
	height: 25px;
	width: 50px !important;
	float: right;
	line-height: 5px;
	margin-top: 10px;
}

.manage-div-svg {
	width: 270px;
	float: right;
	display: none;
	overflow: hidden;
	text-shadow: 0 1px 3px rgba(0, 0, 0, .5);
	box-shadow: inset 0 0 100px rgba(0, 0, 0, .5);
	padding: 10px;
}

.manage-modify-div-svg {
	width: 270px;
	float: right;
	display: none;
	overflow: hidden;
	text-shadow: 0 1px 3px rgba(0, 0, 0, .5);
	box-shadow: inset 0 0 100px rgba(0, 0, 0, .5);
	padding: 10px;
}

.form-control.select-input {
	display: inline;
	width: 25%;
}

.form-control.text-input {
	display: inline;
	width: 75%;
}

.input-time {
	float: left;
	padding-right: 10px;
}

.form-horizontal.time-input{
	overflow:hidden;
}