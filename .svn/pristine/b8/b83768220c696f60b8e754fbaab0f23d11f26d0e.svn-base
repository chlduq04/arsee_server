.qna_main {
	width: 100%;
	height: 100%;
}

.qna-list-name {
	float: left;
	margin-right:10px;
}

.qna-list-comment {
	float: left;
}

.qna-list-result {
	display: block;
	overflow: hidden;
}

.qna-list-question {
	display: block;
	overflow: hidden;
}

ul {
	overflow: hidden;
}

li{
	overflow:hidden;
}

.divide-line {
	width: 100%;
	height: 5px;
	border-bottom: 1px solid darkgray;
	margin-bottom: 25px;
	margin-top: 25px;
}

.qna-submit{
	width:20%;
	float:right;
}

.form-group{
	float: right;
	padding-right: 10px;
	margin-top:10px;
	overflow:hidden;
	text-align: right;
}